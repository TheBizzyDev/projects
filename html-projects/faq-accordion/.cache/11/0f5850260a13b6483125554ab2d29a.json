mn":2},"generated":{"line":1,"column":2652}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":291,"column":4},"generated":{"line":1,"column":2717}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":291,"column":4},"generated":{"line":1,"column":2734}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":296,"column":4},"generated":{"line":1,"column":2735}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":296,"column":4},"generated":{"line":1,"column":2751}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":297,"column":4},"generated":{"line":1,"column":2752}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":297,"column":4},"generated":{"line":1,"column":2769}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":298,"column":4},"generated":{"line":1,"column":2770}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":301,"column":2},"generated":{"line":1,"column":2810}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":303,"column":4},"generated":{"line":1,"column":2843}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":303,"column":4},"generated":{"line":1,"column":2853}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":304,"column":4},"generated":{"line":1,"column":2854}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":304,"column":4},"generated":{"line":1,"column":2863}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":305,"column":4},"generated":{"line":1,"column":2864}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":305,"column":4},"generated":{"line":1,"column":2876}},{"source":"..\\src\\scss\\app.scss","name":null,"original":{"line":306,"column":4},"generated":{"line":1,"column":2877}}],"sources":{"..\\src\\scss\\app.scss":"/*------------------------------------*\\\r\n  #colors\r\n\\*------------------------------------*/\r\n\r\n/* Background Colors */\r\n$backgroundColors-primary:hsl(273, 75%, 66%);\r\n$backgroundColors-secondary:hsl(240, 73%, 65%);\r\n\r\n/* Hover text colors */\r\n$textHover: hsl(14, 88%, 65%);\r\n\r\n/* Question text colors */\r\n$questionActive: hsl(238, 29%, 16%);\r\n$questionInactive: hsl(237, 12%, 33%);\r\n$answerActive: hsl(240, 6%, 50%);\r\n\r\n/* Divider Colors */\r\n$dividerHorizontal: hsl(240, 5%, 91%);\r\n\r\n\r\n/*------------------------------------*\\\r\n  #fonts\r\n\\*------------------------------------*/\r\n\r\n/*font family kumbh */\r\n@import url('https://fonts.googleapis.com/css2?family=Kumbh+Sans:wght@400;700&display=swap');\r\n\r\n/* Font weights */\r\n$font400: 400;\r\n$font700: 700;\r\n\r\n/* Font Family */\r\n$family: 'Kumbh Sans', sans-serif;\r\n\r\n\r\n\r\n\r\n\r\n/*------------------------------------*\\\r\n  #global-Styles\r\n\\*------------------------------------*/\r\n\r\n* {\r\n    box-sizing: border-box;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\nbody {\r\n   margin: 0;\r\n   padding: 0;\r\n   font-size: 16px;\r\n   font-family: $family;\r\n}\r\n\r\nul { list-style: none; }\r\n\r\na { text-decoration: none; }\r\n\r\n\r\n\r\n\r\n\r\n/*------------------------------------*\\\r\n  #utility-classes\r\n\\*------------------------------------*/\r\n.fq-container {\r\n  min-height: 100vh;\r\n  padding: 30px 30px;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background-image: linear-gradient($backgroundColors-primary,$backgroundColors-secondary);\r\n}\r\n\r\n\r\n.fq-card__answer--hide {\r\n  transition: all 2s;\r\n  display: none;\r\n}\r\n\r\n.fq-card__header--bold {\r\n  color: $questionActive;\r\n  font-weight: $font700;\r\n}\r\n\r\n.fq-card__rotate--arrow-up {\r\n  transition: all .5s;\r\n  transform: rotate(180deg);\r\n}\r\n\r\n.fq-card__rotate-arrow-down {\r\n  transform: rotate(-180deg);\r\n  transition: all 1s;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n/*------------------------------------*\\\r\n  #global card styles\r\n\\*------------------------------------*/\r\n\r\n.fq-card {\r\n  width: 850px;\r\n  display: grid;\r\n  padding: 70px;\r\n  grid-template-columns: 1fr 1fr;\r\n  border-radius: 20px;\r\n  background-color: white;\r\n  box-shadow:\r\n  0 2.8px 2.2px rgba(0, 0, 0, 0.034),\r\n  0 6.7px 5.3px rgba(0, 0, 0, 0.048),\r\n  0 12.5px 10px rgba(0, 0, 0, 0.06),\r\n  0 22.3px 17.9px rgba(0, 0, 0, 0.072),\r\n  0 41.8px 33.4px rgba(0, 0, 0, 0.086),\r\n  0 100px 80px rgba(0, 0, 0, 0.12)\r\n  ;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n/*------------------------------------*\\\r\n  #header placement and div width sizing for \r\n  card\r\n\\*------------------------------------*/\r\n\r\n.fq-card {\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.fq-card__header {\r\n  padding-bottom: 30px;\r\n}\r\n\r\n.fq-card__faqs {\r\n  width: 100%;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n/*------------------------------------*\\\r\n  #faq detail styles\r\n\\*------------------------------------*/\r\n\r\n// Background floating image\r\n.fq-card__image img:nth-child(1){\r\n  display: block;\r\n  position: absolute;\r\n  top: 45px;\r\n  left: -70px;\r\n  width: 420px;\r\n  margin-top: auto;\r\n  margin-bottom: auto;\r\n  z-index: 2;\r\n  animation: float 4s ease-in-out infinite;\r\n}\r\n\r\n// image box on the container\r\n.fq-card__image img:nth-child(2) {\r\n  position: absolute;\r\n  top: 155px;\r\n  left: -125px;\r\n  z-index: 1000;\r\n  animation: float 4s ease-in-out infinite;\r\n}\r\n\r\n// background static image\r\n.fq-card__image img:nth-child(3){\r\n  // display: none;\r\n  position: absolute;\r\n  top: -240px;\r\n  left: -450px;\r\n  width: 800px;\r\n}\r\n\r\n// Animage float images\r\n@keyframes float {\r\n\t0% {\r\n\t\ttransform: translatey(0px);\r\n\t}\r\n\t50% {\r\n\t\ttransform: translatey(-10px);\r\n\t}\r\n\t100% {\r\n\t\ttransform: translatey(0px);\r\n\t}\r\n}\r\n\r\n.fq-card__question {\r\n  border-bottom: 1px solid $dividerHorizontal;\r\n}\r\n\r\n.fq-card-group {\r\n  display: grid;\r\n  grid-template-columns: 9fr 1fr;\r\n  align-items: center;\r\n}\r\n\r\n.fq-card-group p:first-child {\r\n  color: $questionInactive;\r\n  font-size: .9rem;\r\n  padding: 15px 0;\r\n}\r\n\r\n.fq-card-group,\r\n.fq-card-group p:first-child:hover {\r\n  cursor: pointer;\r\n  color: $textHover;\r\n}\r\n\r\n.fq-card-group img {\r\n  justify-self: center;\r\n}\r\n\r\n.fq-card__answer {\r\n  font-size: .9rem;\r\n  color: $answerActive;\r\n  padding-bottom: 10px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n/*------------------------------------*\\\r\n  #Media Queries\r\n\\*------------------------------------*/\r\n@media only screen and (max-width: 750px) {\r\n  \r\n  // Faq heading text center\r\n  .fq-card__header {\r\n    text-align: center;\r\n  }\r\n  \r\n  // Change grid columns to 100% width\r\n  .fq-card {\r\n    grid-template-columns: 1fr;\r\n    grid-template-rows: 1fr 4fr;\r\n    padding: 70px 30px;\r\n    overflow: visible;\r\n  }\r\n  // Remove image\r\n  .fq-card__image img:nth-child(3) {\r\n    display: none;\r\n  }\r\n  \r\n  // Adjust image size\r\n  .fq-card__image img:nth-child(1) {\r\n    position: absolute;\r\n    width: 300px;\r\n    top: -100px;\r\n    right: 0;\r\n    left: 0;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    animation: float 4s ease-in-out infinite;\r\n  }\r\n\r\n  .fq-card__image img:nth-child(2) {\r\n    position: absolute;\r\n    width: 120px;\r\n    top: -12px;\r\n    right: -55px;\r\n    left: -250px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    animation: float 4s ease-in-out infinite;\r\n  }\r\n\r\n  .fq-card-group img {\r\n    justify-self: end;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 450px) {\r\n\r\n  // adjusting images sizes\r\n  .fq-card {\r\n    grid-template-rows: 1fr;\r\n  }\r\n  .fq-card__image img:nth-child(1) {\r\n    position: absolute;\r\n    width: 200px;\r\n    top: -100px;\r\n    right: 0;\r\n    left: 0;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    animation: float 4s ease-in-out infinite;\r\n  }\r\n\r\n  .fq-card__image img:nth-child(2) {\r\n    position: absolute;\r\n    width: 70px;\r\n    top: -38px;\r\n    right: -135px;\r\n    left: -260px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    animation: float 4s ease-in-out infinite;\r\n  }\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"71fc61480732c742648cf54de77c9323","cacheData":{"env":{}}}