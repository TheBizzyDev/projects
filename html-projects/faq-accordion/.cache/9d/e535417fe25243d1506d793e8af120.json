/app.scss","name":null,"original":{"line":298,"column":4},"generated":{"line":1,"column":2752}},{"source":"../src/scss/app.scss","name":null,"original":{"line":298,"column":4},"generated":{"line":1,"column":2769}},{"source":"../src/scss/app.scss","name":null,"original":{"line":299,"column":4},"generated":{"line":1,"column":2770}},{"source":"../src/scss/app.scss","name":null,"original":{"line":302,"column":2},"generated":{"line":1,"column":2810}},{"source":"../src/scss/app.scss","name":null,"original":{"line":304,"column":4},"generated":{"line":1,"column":2843}},{"source":"../src/scss/app.scss","name":null,"original":{"line":304,"column":4},"generated":{"line":1,"column":2853}},{"source":"../src/scss/app.scss","name":null,"original":{"line":305,"column":4},"generated":{"line":1,"column":2854}},{"source":"../src/scss/app.scss","name":null,"original":{"line":305,"column":4},"generated":{"line":1,"column":2863}},{"source":"../src/scss/app.scss","name":null,"original":{"line":306,"column":4},"generated":{"line":1,"column":2864}},{"source":"../src/scss/app.scss","name":null,"original":{"line":306,"column":4},"generated":{"line":1,"column":2876}},{"source":"../src/scss/app.scss","name":null,"original":{"line":307,"column":4},"generated":{"line":1,"column":2877}}],"sources":{"../src/scss/app.scss":"/*------------------------------------*\\\n  #colors\n\\*------------------------------------*/\n\n/* Background Colors */\n$backgroundColors-primary:hsl(273, 75%, 66%);\n$backgroundColors-secondary:hsl(240, 73%, 65%);\n\n/* Hover text colors */\n$textHover: hsl(14, 88%, 65%);\n\n/* Question text colors */\n$questionActive: hsl(238, 29%, 16%);\n$questionInactive: hsl(237, 12%, 33%);\n$answerActive: hsl(240, 6%, 50%);\n\n/* Divider Colors */\n$dividerHorizontal: hsl(240, 5%, 91%);\n\n\n/*------------------------------------*\\\n  #fonts\n\\*------------------------------------*/\n\n/*font family kumbh */\n@import url('https://fonts.googleapis.com/css2?family=Kumbh+Sans:wght@400;700&display=swap');\n\n/* Font weights */\n$font400: 400;\n$font700: 700;\n\n/* Font Family */\n$family: 'Kumbh Sans', sans-serif;\n\n\n\n\n\n/*------------------------------------*\\\n  #global-Styles\n\\*------------------------------------*/\n\n* {\n    box-sizing: border-box;\n    margin: 0;\n    padding: 0;\n}\n\nbody {\n   margin: 0;\n   padding: 0;\n   font-size: 16px;\n   font-family: $family;\n}\n\nul { list-style: none; }\n\na { text-decoration: none; }\n\n\n\n\n\n/*------------------------------------*\\\n  #utility-classes\n\\*------------------------------------*/\n.fq-container {\n  min-height: 100vh;\n  padding: 30px 30px;\n  width: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background-image: linear-gradient($backgroundColors-primary,$backgroundColors-secondary);\n}\n\n\n.fq-card__answer--hide {\n  transition: all 2s;\n  display: none;\n}\n\n.fq-card__header--bold {\n  color: $questionActive;\n  font-weight: $font700;\n}\n\n.fq-card__rotate--arrow-up {\n  transition: all .5s;\n  transform: rotate(180deg);\n}\n\n.fq-card__rotate-arrow-down {\n  transform: rotate(-180deg);\n  transition: all 1s;\n}\n\n\n\n\n\n/*------------------------------------*\\\n  #global card styles\n\\*------------------------------------*/\n\n.fq-card {\n  width: 850px;\n  display: grid;\n  padding: 70px;\n  grid-template-columns: 1fr 1fr;\n  border-radius: 20px;\n  background-color: white;\n  box-shadow:\n  0 2.8px 2.2px rgba(0, 0, 0, 0.034),\n  0 6.7px 5.3px rgba(0, 0, 0, 0.048),\n  0 12.5px 10px rgba(0, 0, 0, 0.06),\n  0 22.3px 17.9px rgba(0, 0, 0, 0.072),\n  0 41.8px 33.4px rgba(0, 0, 0, 0.086),\n  0 100px 80px rgba(0, 0, 0, 0.12)\n  ;\n}\n\n\n\n\n\n/*------------------------------------*\\\n  #header placement and div width sizing for \n  card\n\\*------------------------------------*/\n\n.fq-card {\n  position: relative;\n  overflow: hidden;\n}\n\n.fq-card__header {\n  padding-bottom: 30px;\n}\n\n.fq-card__faqs {\n  width: 100%;\n}\n\n\n\n\n\n/*------------------------------------*\\\n  #faq detail styles\n\\*------------------------------------*/\n\n// Background floating image\n.fq-card__image img:nth-child(1){\n  display: block;\n  position: absolute;\n  top: 45px;\n  left: -70px;\n  width: 420px;\n  margin-top: auto;\n  margin-bottom: auto;\n  z-index: 2;\n  animation: float 4s ease-in-out infinite;\n}\n\n// image box on the container\n.fq-card__image img:nth-child(2) {\n  position: absolute;\n  top: 155px;\n  left: -125px;\n  z-index: 1000;\n  animation: float 4s ease-in-out infinite;\n}\n\n// background static image\n.fq-card__image img:nth-child(3){\n  // display: none;\n  position: absolute;\n  top: -240px;\n  left: -450px;\n  width: 800px;\n}\n\n// Animage float images\n@keyframes float {\n\t0% {\n\t\ttransform: translatey(0px);\n\t}\n\t50% {\n\t\ttransform: translatey(-10px);\n\t}\n\t100% {\n\t\ttransform: translatey(0px);\n\t}\n}\n\n.fq-card__question {\n  border-bottom: 1px solid $dividerHorizontal;\n}\n\n.fq-card-group {\n  display: grid;\n  grid-template-columns: 9fr 1fr;\n  align-items: center;\n}\n\n.fq-card-group p:first-child {\n  color: $questionInactive;\n  font-size: .9rem;\n  padding: 15px 0;\n}\n\n.fq-card-group,\n.fq-card-group p:first-child:hover {\n  cursor: pointer;\n  color: $textHover;\n}\n\n.fq-card-group img {\n  justify-self: center;\n  \n}\n\n.fq-card__answer {\n  font-size: .9rem;\n  color: $answerActive;\n  padding-bottom: 10px;\n}\n\n\n\n\n\n\n/*------------------------------------*\\\n  #Media Queries\n\\*------------------------------------*/\n@media only screen and (max-width: 750px) {\n  \n  // Faq heading text center\n  .fq-card__header {\n    text-align: center;\n  }\n  \n  // Change grid columns to 100% width\n  .fq-card {\n    grid-template-columns: 1fr;\n    grid-template-rows: 1fr 4fr;\n    padding: 70px 30px;\n    overflow: visible;\n  }\n  // Remove image\n  .fq-card__image img:nth-child(3) {\n    display: none;\n  }\n  \n  // Adjust image size\n  .fq-card__image img:nth-child(1) {\n    position: absolute;\n    width: 300px;\n    top: -100px;\n    right: 0;\n    left: 0;\n    margin-left: auto;\n    margin-right: auto;\n    animation: float 4s ease-in-out infinite;\n  }\n\n  .fq-card__image img:nth-child(2) {\n    position: absolute;\n    width: 120px;\n    top: -12px;\n    right: -55px;\n    left: -250px;\n    margin-left: auto;\n    margin-right: auto;\n    animation: float 4s ease-in-out infinite;\n  }\n\n  .fq-card-group img {\n    justify-self: end;\n  }\n}\n\n@media only screen and (max-width: 450px) {\n\n  // adjusting images sizes\n  .fq-card {\n    grid-template-rows: 1fr;\n  }\n  .fq-card__image img:nth-child(1) {\n    position: absolute;\n    width: 200px;\n    top: -100px;\n    right: 0;\n    left: 0;\n    margin-left: auto;\n    margin-right: auto;\n    animation: float 4s ease-in-out infinite;\n  }\n\n  .fq-card__image img:nth-child(2) {\n    position: absolute;\n    width: 70px;\n    top: -38px;\n    right: -135px;\n    left: -260px;\n    margin-left: auto;\n    margin-right: auto;\n    animation: float 4s ease-in-out infinite;\n  }\n}\n\n\n\n\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"528f2075bd7a963e9c7838b82d05a1a3","cacheData":{"env":{}}}